\"}\r\n              variants={backdropVariants}\r\n              transition={expandingTransition}>\r\n\r\n\r\n            </BackDrop>           \r\n            {active === \"signin\" && (\r\n              <HeaderContainer>\r\n                <HeaderText>\r\n                  <FormattedMessage\r\n                    id = \"login.Welcome\"\r\n                  />\r\n                </HeaderText>\r\n                <HeaderText>\r\n                  <FormattedMessage\r\n                    id = \"login.Back\"\r\n                  />  \r\n                </HeaderText>\r\n                <SmallText>\r\n                  <FormattedMessage\r\n                    id = \"login.SmallText\"\r\n                  />\r\n                </SmallText>\r\n              </HeaderContainer>\r\n            )}\r\n            {active === \"signup\" && (\r\n              <HeaderContainer>\r\n                <HeaderText>\r\n                  <FormattedMessage\r\n                    id = \"signup.create\"\r\n                  />  \r\n                </HeaderText>\r\n                <HeaderText>\r\n                  <FormattedMessage\r\n                    id = \"signup.account\"\r\n                  />\r\n                </HeaderText>\r\n                <SmallText>\r\n                  <FormattedMessage\r\n                    id = \"signup.smallText\"\r\n                  />\r\n                </SmallText>\r\n              </HeaderContainer>\r\n            )}\r\n          </TopContainer>\r\n          <InnerContainer>\r\n            {active === \"signin\" && <LoginForm />}\r\n            {active === \"signup\" && <SignupForm />}\r\n          </InnerContainer>\r\n        </BoxContainer>\r\n      </AccountContext.Provider>\r\n    </div> \r\n\r\n  );\r\n}\r\n\r\nexport default AccountBox;","C:\\Users\\Shahwaiz Nathani\\Desktop\\mutrajim-fyp\\Mehnat-o-Musshakat\\src\\components\\accountBox\\accountContext.js",[],"C:\\Users\\Shahwaiz Nathani\\Desktop\\mutrajim-fyp\\Mehnat-o-Musshakat\\src\\componen